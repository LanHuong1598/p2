    var district_ = (string)ViewBag.districtname;
    var province_ = (string)ViewBag.provincename;
    var enrollment_ = (string)ViewBag.enrollmentname;

}

@{
    string ngaysinhdate = null;
    if (user.date_of_birth != null)
    {
        string ngaysinhStr = user.date_of_birth.ToString();
        DateTime date = (Convert.ToDateTime(ngaysinhStr));
        string day = "-" + date.Day, month = "-" + date.Month;
        if (date.Day < 10)
        {
            day = "-0" + date.Day;

        }
        if (date.Month < 10)
        {
            month = "-0" + date.Month;

        }

        ngaysinhdate = date.Year + month + day;

    }
}
