        <!-- Bootstrap -->
        <script src="~/Areas/Admin/Content/table/bootstrap.bundle.min.js"></script>
        <!-- Datatables -->
        <script src="~/Areas/Admin/Content/table/jquery.dataTables.min.js"></script>
        <script src="~/Areas/Admin/Content/table/dataTables.bootstrap.min.js"></script>
        <script src="~/Areas/Admin/Content/table/dataTables.buttons.min.js"></script>
        <script src="~/Areas/Admin/Content/table/dataTables.keyTable.min.js"></script>
        <script src="~/Areas/Admin/Content/table/dataTables.responsive.min.js"></script>
        <script src="~/Areas/Admin/Content/table/responsive.bootstrap.js"></script>
        <script src="~/Areas/Admin/Content/table/dataTables.scroller.min.js"></script>*@





    <script>

        window.onload = function () {

            $.get("/Admin/HomeAdmin/getNameAdmin", function (data) {
                $("#name_admin").html("Đăng xuất("+data+")")
            });
        }
       
    </script>









</body>

</html>