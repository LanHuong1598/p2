<body>
    <div class="app-container app-theme-white body-tabs-shadow fixed-sidebar fixed-header">
        <div class="app-header header-shadow">
            <div class="app-header__logo">
                <a>
                    <img style="" class="rounded-circle" width="45" src="~/Areas/Admin/Content/assets/images/logo.png" />
                   
                </a>
                <div class="header__pane ml-auto">
                    <div>
                        <button type="button" class="hamburger close-sidebar-btn hamburger--elastic"
                                data-class="closed-sidebar">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
            <div class="app-header__mobile-menu">
                <div>
                    <button type="button" class="hamburger hamburger--elastic mobile-toggle-nav">
                        <span class="hamburger-box">
                            <span class="hamburger-inner"></span>
                        </span>
                    </button>
                </div>
            </div>
            <div class="app-header__menu">
                <span>
                    <button type="button"
                            class="btn-icon btn-icon-only btn btn-primary btn-sm mobile-toggle-header-nav">
                        <span class="btn-icon-wrapper">
                            <i class="fa fa-ellipsis-v fa-w-6"></i>
                        </span>
                    </button>
                </span>
            </div>
            <div class="app-header__content">

                <h2 style="margin-left:100px">Hệ thống tuyển sinh Học viện Kỹ thuật Quân sự</h2>



                <div class="app-header-right">
                    <a id="name_admin" href="/Admin/HomeAdmin/LogoutAdmin">Đăng xuất(@ViewBag.nameadmin) </a>
                </div>
            </div>

        </div>

        <div class="app-main">
            <div class="app-sidebar sidebar-shadow">
                <div class="app-header__logo">
                    <a>
                        <img class="rounded-circle" width="50" src="~/Areas/Admin/Content/assets/images/logo.png" />
                    </a>
                     
                    <div class="header__pane ml-auto">
                        <div>
                            <button type="button" class="hamburger close-sidebar-btn hamburger--elastic"
                                    data-class="closed-sidebar">
                                <span class="hamburger-box">
                                    <span class="hamburger-inner"></span>
                                </span>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="app-header__mobile-menu">
                    <div>
                        <button type="button" class="hamburger hamburger--elastic mobile-toggle-nav">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
                <div class="app-header__menu">
                    <span>
                        <button type="button"
                                class="btn-icon btn-icon-only btn btn-primary btn-sm mobile-toggle-header-nav">
                            <span class="btn-icon-wrapper">
                                <i class="fa fa-ellipsis-v fa-w-6"></i>
                            </span>
                        </button>
                    </span>
                </div>
                <div class="scrollbar-sidebar">
                    <div class="app-sidebar__inner">
                        <ul class="vertical-nav-menu">
                            <li class="app-sidebar__heading"> <h4>Bảng điều khiển </h4></li>
                            <li>
                                @{
                                    if (ViewBag.layout == "dashboard")
                                    {
                                <a id="dashboard" href="/Admin/HomeAdmin/Dashboard" class="mm-active">
                                    <i class="metismenu-icon pe-7s-culture"></i>
                                    Trang chủ
                                </a>
                                    }
                                    else
                                    {
                                <a id="dashboard" href="/Admin/HomeAdmin/Dashboard">
                                    <i class="metismenu-icon pe-7s-culture"></i>
                                    Trang chủ
                                </a>
                                    }
                                }

                            </li>

                            <li>
                                @{
                                    if (ViewBag.layout == "history")
                                    {
                                        <a id="history" href="/Admin/HomeAdmin/History_Update_Student" class="mm-active">
                                            <i class="metismenu-icon pe-7s-bookmarks"></i>
                                            Lịch sử cập nhật
                                        </a>
                                    }
                                    else
                                    {
                                        <a id="history" href="/Admin/HomeAdmin/History_Update_Student">
                                            <i class="metismenu-icon pe-7s-bookmarks"></i>
                                            Lịch sử cập nhật
                                        </a>
                                    }
                                }

                            </li>
                            <li>
                                @{
                                    if (ViewBag.layout == "duplicate")
                                    {
                                        <a id="duplicate" href="/Admin/HomeAdmin/Duplicate_IdentityCard" class="mm-active">
                                            <i class="metismenu-icon pe-7s-note"></i>
                                            Danh sách trùng CMND
                                        </a>
                                    }
                                    else
                                    {
                                        <a id="duplicate" href="/Admin/HomeAdmin/Duplicate_IdentityCard">
                                            <i class="metismenu-icon pe-7s-note"></i>
                                            Danh sách trùng CMND
                                        </a>
                                    }
                                }
                                
                            </li>
                            <li>
                                @{
                                    if (ViewBag.layout == "account")
                                    {
                                        <a href="/Admin/AccountStudent/Index" class="mm-active">
                                            <i class="metismenu-icon pe-7s-user"></i>
                                            Danh sách tài khoản
                                        </a>
                                    }
                                    else
                                    {
                                        <a href="/Admin/AccountStudent/Index">
                                            <i class="metismenu-icon pe-7s-user"></i>
                                            Danh sách tài khoản
                                        </a>
                                    }
                                }

                            </li>

                        </ul>
                    </div>
                </div>
            </div>
            <div class="app-main__outer">
                @RenderBody()
                <div class="app-wrapper-footer">
                    <div class="app-footer">
                        <div class="app-footer__inner">
                            <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12">
                                Copyright © 2021 Học viện Kỹ thuật Quân sự
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
    <script type="text/javascript" src="~/Areas/Admin/Content/assets/scripts/main.js"></script>


    @*<script type="text/javascript" src="https://cdn.datatables.net/v/bs4/dt-1.10.25/datatables.min.js"></script>*@


    <!-- jQuery -->
